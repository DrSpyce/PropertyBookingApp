@model IEnumerable<PropertyListingModel>

@{
}

@foreach(var item in Model)
{
    <div>
        <h4>@item.Name</h4>
        <p>@item.Blurb</p>
        <p>@item.Location</p>
        <p>@item.CostPerNight</p>
        <a asp-controller="PropertyListing" asp-action="ViewPropertyDetails" asp-route-id="@item.Id">Details</a>
    </div>
}